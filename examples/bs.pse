declare length, i, j, tmp : integer

length <- 1000

declare unsorted : array[1:length] of integer

for i <- 1 to length
    unsorted[i] <- 100- - i
next i

for i <- 1 to length - 1
    for j <- 1 to length - i
        if unsorted[j] > unsorted[j + 1] then
            tmp <- unsorted[j]
            unsorted[j] <- unsorted[j + 1]
            unsorted[j + 1] <- tmp
        endif
    next j
next i

